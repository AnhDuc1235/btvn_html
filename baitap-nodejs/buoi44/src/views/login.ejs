<link rel="stylesheet" href="/assets/style.css">
<form method="POST" action="/login" class="form">
    <h1 class="form-text">Login</h1>
    <% if (failed) { %>
        <h3><%= failed %></h3>
    <% } %>
    <input name="email" type="email" placeholder="anhduc@gmail.com"/>
    <% if (emailError) { %>
        <h3><%= emailError %></h3>
    <% } %>
    <input name="password" type="password" placeholder="duc123"/>
    <% if (passwordError) { %>
        <h3><%= passwordError %></h3>
    <% } %>
    <button type="submit">Đăng nhập</button>
</form>


